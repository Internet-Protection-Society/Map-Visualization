  "dependencies": {
    "axios": "^0.21.1",
    "d3": "next",
    "prop-types": "^15.7.2",
    "react": "next",
    "react-dom": "next"
  },
  "resolutions": {
    "acorn": "^5.7.4",
    "minimist": "^1.2.2",
    "serialize-javascript": "^2.1.2",
    "set-value": "^2.0.1",
